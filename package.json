{
  "name": "gain-solutions-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=6144 --trace-warnings --inspect --trace-uncaught' rm -rf .next && next dev",
    "build": "pnpm run db-generate && rm -rf .next && NODE_ENV=production next build && pnpm run db-push",
    "build-test": "rm -rf .next && NODE_ENV=test next build",
    "clean-install": "rm -rf node_modules pnpm-lock.yaml yarn.lock bun.lockb package-lock.json && pnpm install",
    "clean-install-pmy": "rm -rf node_modules pnpm-lock.yaml yarn.lock bun.lockb package-lock.json && yarn",
    "clean-install-pmb": "rm -rf node_modules pnpm-lock.yaml yarn.lock bun.lockb package-lock.json && bun install",
    "clean-build": "pnpm run clean-install && pnpm run db-generate && pnpm build",
    "clean-build-pmy": "yarn run clean-install-pmy && yarn run db-generate-pmy && yarn run build",
    "clean-build-pmb": "bun run clean-install-pmb && bun run db-generate-pmb && bun run build",
    "start": "NODE_ENV=production next start",
    "start-local": "NODE_ENV=production next start",
    "start-test-local": "NODE_ENV=test next start",
    "pre-commit": "lint-staged --allow-empty",
    "format": "prettier --write .",
    "lint": "next lint",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "db-generate": "pnpm prisma generate",
    "db-generate-pmy": "yarn prisma generate",
    "db-generate-pmb": "bun prisma generate",
    "db-pull": "dotenv -c $NODE_ENV -- pnpm prisma db pull && pnpm run db-generate",
    "db-pull-pmy": "dotenv -c $NODE_ENV -- yarn prisma db pull && yarn run db-generate-pmy",
    "db-pull-pmb": "dotenv -c $NODE_ENV -- bun prisma db pull && bun run db-generate-pmb",
    "db-push": "dotenv -c $NODE_ENV -- pnpm prisma db push",
    "db-push-pmy": "dotenv -c $NODE_ENV -- yarn prisma db push",
    "db-push-pmb": "dotenv -c $NODE_ENV -- bun prisma db push",
    "db-benchmark": "dotenv -c $NODE_ENV -- ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seeder/benchmark.ts",
    "db-seed": "dotenv -c $NODE_ENV -- ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts"
  },
  "dependencies": {
    "@vercel/analytics": "^1.3.2",
    "@vercel/speed-insights": "^1.0.14",
    "bcrypt": "^5.1.1",
    "jose": "^5.9.6",
    "jsonwebtoken": "^9.0.2",
    "next": "14.x",
    "prisma": "5.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.3",
    "@prisma/client": "5.8.1",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.1.2",
    "@types/bcrypt": "^5.0.2",
    "@types/humanize-duration": "^3.27.4",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "20.9.2",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.50.0",
    "eslint-config-next": "^13.5.3",
    "eslint-plugin-prettier": "^5.0.0",
    "humanize-duration": "^3.32.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.x"
  }
}
